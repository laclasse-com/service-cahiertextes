describe "Float#CONSTANTS", ->
  it "constants", ->
    expect( R.Float.DIG        ).toEqual 15
    expect( R.Float.EPSILON    ).toEqual eval("0.0000000000000002220446049250313080847263336181640625")
    expect( R.Float.MANT_DIG   ).toEqual 53
    expect( R.Float.MAX_10_EXP ).toEqual 308
    expect( R.Float.MIN_10_EXP ).toEqual -307
    expect( R.Float.MAX_EXP    ).toEqual 1024
    expect( R.Float.MIN_EXP    ).toEqual -1021
    expect( R.Float.MAX        ).toEqual eval(".0")
    expect( R.Float.RADIX      ).toEqual 2

